         <property name="styleSheet">
          <string notr="true">/* Style global pour QComboBox */
QComboBox {
 background-color: rgb(0, 85, 164);
   /* background-color: rgb(0, 85, 164);  /* Bleu plus doux et clair que le bleu des boutons */
    color: white;  /* Texte en blanc pour une bonne lisibilité */
    border: 2px solid rgb(0, 102, 204);  /* Bordure qui s'harmonise avec le bleu des boutons */
    border-radius: 0px;  /* Coins carrés (pas d'arrondi) */
    padding: 5px;  /* Espacement interne pour rendre la zone de texte confortable */
    font-size: 14px;  /* Taille de police claire */
}

/* Style pour la liste déroulante (éléments du QComboBox) */
QComboBox QAbstractItemView {
    background-color: rgb(0, 102, 204);  /* Couleur de fond de la liste déroulante (même bleu que les boutons) */
    color: white;  /* Texte en blanc pour une meilleure visibilité */
    selection-background-color: rgb(0, 85, 164);  /* Bleu plus clair au survol */
    selection-color: black;  /* Texte noir lors de la sélection */
}

/* Style des éléments de la liste au survol */
QComboBox QAbstractItemView::item:hover {
    background-color: rgb(0, 125, 255);  /* Bleu légèrement plus clair pour le survol */
    color: black;  /* Texte noir au survol pour contraste */
}

/* Si le QComboBox est désactivé */
QComboBox:disabled {
    background-color: rgb(204, 204, 204);  /* Gris clair si désactivé */
    color: rgb(128, 128, 128);  /* Texte gris pour l'état désactivé */
    border: 2px solid rgb(169, 169, 169);  /* Bordure grise si désactivé */
}
</string>
