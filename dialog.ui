    <string notr="true">QPushButton {
	image: url(:/Downloads/png-clipart-computer-icons-login-graphics-illustration-power-steering-logo-silhouette.png);
    border-radius: 10px; /* Coins arrondis */
    background: #F0F0F0; /* Fond gris clair */
    color: #2C3E50; /* Couleur du texte bleu-gris foncé */
    padding: 12px 20px; /* Espacement plus grand pour une zone de clic plus large */
    border: 2px solid black; /* Contour noir */
    font-weight: bold; /* Texte en gras pour mettre en valeur */
    font-size: 16px; /* Taille de police légèrement plus grande */
    transition: background-color 0.3s, box-shadow 0.3s, transform 0.2s; /* Transitions douces */
}

QPushButton:hover {
    background: rgba(41, 128, 185, 0.15); /* Bleu clair au survol */
    box-shadow: 0px 6px 20px rgba(0, 0, 0, 0.3); /* Ombre plus prononcée pour la profondeur */
    transform: translateY(-3px); /* Effet de soulèvement */
}

QPushButton:pressed {
    background: rgba(41, 128, 185, 0.25); /* Bleu légèrement plus foncé lorsque pressé */
    box-shadow: inset 0px 6px 12px rgba(0, 0, 0, 0.2); /* Effet d'ombre intérieure */
    transform: translateY(1px); /* Effet de légère dépression */
}
